{
    "modules" : {
        "module":
        [
            {% if local_files|length > 1 %}
            {% for module in local_files[:-1] %}{
                "name" : "{{module.name}}",
                "revision" : "{{module.revision}}",
                "uri" : "{{module.uri}}",
                "kind" : "{{module.kind}}"
            },

            {% endfor %}{
                "name" : "{{local_files[-1].name}}",
                "revision" : "{{local_files[-1].revision}}",
                "uri" : "{{local_files[-1].uri}}",
                "kind" : "{{local_files[-1].kind}}"
            }
            {% endif %}

            {% if remote_files|length > 1%},
            {% for module in remote_files[:-1] %}{
                "name" : "{{module.name}}",
                "revision" : "{{module.revision}}",
                "uri" : "{{module.uri}}",
                "kind" : "{{module.kind}}"
            },

            {% endfor %}{
                "name" : "{{remote_files[-1].name}}",
                "revision" : "{{remote_files[-1].revision}}",
                "uri" : "{{remote_files[-1].uri}}",
                "kind" : "{{remote_files[-1].kind}}"
            }
            {% endif %}
        ]
    },

    "bundle" : {
        "name" : "{{bundle.name}}",
        "bundle-version" : {
            "major-version-number" : "{{bundle.major}}",
            "minor-version-number" : "{{bundle.minor}}",
            "patch-level" : "{{bundle.patch}}"
        },
        "ydk-version" : {
            "major-version-number" : "{{ydk_version.major}}",
            "minor-version-number" : "{{ydk_version.minor}}",
            "patch-level" : "{{ydk_version.patch}}"
        }
        {% if dependencies is defined %}
        "bundle-dependencies" : [
            {% for dependency in dependencies[:-1] %}{
                "name" : {{dependency.name}},
                "bundle-version" : {
                    "major-version-number" : {{dependency.bundle_version.major}},
                    "minor-version-number" : {{dependency.bundle_version.minor}},
                    "patch-level" : {{dependency.bundle_version.patch}}
                },
                "ydk-version" : {
                    "major-version-number" : {{dependency.ydk_version.major}},
                    "minor-version-number" : {{dependency.ydk_version.minor}},
                    "patch-level" : {{dependency.ydk_version.patch}}
                }
            },

            {% endfor %}{
                "name" : {{dependencies[-1].name}},
                "bundle-version" : {
                    "major-version-number" : {{dependencies[-1].bundle_version.major}},
                    "minor-version-number" : {{dependencies[-1].bundle_version.minor}},
                    "patch-level" : {{dependencies[-1].bundle_version.patch}}
                },
                "ydk_version" : {
                    "major-version-number" : {{dependencies[-1].ydk_version.major}},
                    "minor-version-number" : {{dependencies[-1].ydk_version.minor}},
                    "patch-level" : {{dependencies[-1].ydk_version.patch}}
                }
            }
        ]
        {% endif %}
    }
}
